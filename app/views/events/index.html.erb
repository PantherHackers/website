<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Start</th>
      <th>End</th>
      <th>Posted</th>
      <th>Id</th>
      <th>Author</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.body %></td>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        <td><%= event.posted %></td>
        <td><%= event.id %></td>
        <td><%= event.author %></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>
